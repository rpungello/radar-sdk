{
    "name": "rpungello/radar-sdk",
    "description": "PHP SDK for interfacing with radar.com",
    "keywords": [
        "rpungello",
        "radar-sdk"
    ],
    "homepage": "https://github.com/rpungello/radar-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Pungello",
            "email": "rob@rpun.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Rpungello\\RadarSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rpungello\\RadarSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
